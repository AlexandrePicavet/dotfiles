[include]
	path = .gitconfig-personal
[core]
	pager = delta
[commit]
	gpgsign = true
[pull]
	rebase = true
[push]
	autoSetupRemote = true
[merge]
	conflictstyle = diff3
[diff]
	colorMoved = default
[interactive]
	diffFilter = delta --color-only
[delta]
	navigate = true
	light = false
	side-by-side = true
	line-numbers = true
